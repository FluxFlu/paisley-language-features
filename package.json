{
    "name": "paisley-language-features",
    "displayName": "Paisley Language Features",
    "description": "Provides language support for Paisley.",
    "version": "1.0.0",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "author": "Missy (the God)",
    "publisher": "Missy",
    "categories": [
      "Programming Languages"
    ],
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/FluxFlu/paisley-language-features.git"
    },
    "bugs": {
      "url": "https://github.com/FluxFlu/paisley-language-features/issues"
    },
    "contributes": {
        "languages": [
            {
                "id": "paisley",
                "aliases": [
                    "Paisley",
                    "sly"
                ],
                "extensions": [
                    ".sly"
                ]
            }
        ],
        "grammars": [
            {
                "language": "paisley",
                "scopeName": "source.sly",
                "path": "./syntaxes/paisley.tmLanguage"
            }
        ]
    }
}